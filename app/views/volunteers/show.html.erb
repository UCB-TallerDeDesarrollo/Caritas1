<h1>Ver Voluntario</h1>

<table>

  <tr>
    <td>
      <b>Nombre:</b> </td><td>
      <%=h @volunteer.name %>
    </td>
  </tr>
  <tr>
    <td>
      <b>Apellido Paterno:</b> </td><td>
      <%=h @volunteer.last_name %>
    </td>
  </tr>
  <tr>
    <td>

      <b>Apellido Materno:</b></td><td>
      <%=h @volunteer.second_last_name %>

    </td>
  </tr>
  <tr>
    <td>
      <b>Profesion:</b></td><td>
      <%=h @volunteer.profession %>

    </td>
  </tr>
  <tr>
    <td>
      <b>Telefono:</b></td><td>
      <%=h @volunteer.phone_number %>
    </td>
  </tr>
  <tr>
    <td>

      <b>Celular:</b></td><td>
      <%=h @volunteer.movil %>
    </td>
  </tr>
  <tr>
    <td>

      <b>Fecha de Nacimiento:</b></td><td>
      <%=h @volunteer.date_of_birth %>
    </td>
  </tr>
  <tr>
    <td>

      <b>Genero:</b></td><td>
      <%=h @volunteer.genre %>
    </td>
  </tr>
  <tr>
    <td>

      <b>Cargo:</b></td><td>
      <%=h @volunteer.position %>
    </td>
  </tr>
  <tr>
    <td>

      <b>Email:</b></td><td>
      <%=h @volunteer.email %>
    </td>
  </tr>

  
  <tr align="right">
    <td colspan="2">
      <%= link_to 'Editar', edit_volunteer_path(@volunteer) %> |
      <%= link_to 'Volver', volunteers_path %>
    </td>
  </tr>
</table>
