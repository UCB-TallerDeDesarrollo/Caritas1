<h1>Editar Vicaria</h1>

<% form_for(@vicariou) do |f| %>
  <%= f.error_messages %>
<table>
<tr><td><%= f.label :name_vicariou %></td><td><%= f.text_field :name_vicariou %></td></tr>
<tr><td><%= f.label :ubication %></td><td><%= f.text_field :ubication %></td></tr>
<tr><td><%= f.label :transport %></td><td><%= f.text_field :transport %></td></tr>
<tr><td><%= f.label :telephone %></td><td><%= f.text_field :telephone %></td></tr>
<tr><td><%= f.label :vicar_name %></td><td><%= f.text_field :vicar_name %></td></tr>
  <tr>
      <td>*  Estos campos son obligatorios</td>
    </tr>
</table>

  
  <p>
    <%= f.submit 'Editar' %>
  </p>
<% end %>

<%= link_to 'Ver', @vicariou %> |
<%= link_to 'Cancelar', vicarious_path %>