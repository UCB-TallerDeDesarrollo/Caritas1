<h1>New social_work</h1>

<% form_for(@social_work) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :Nombre %><br />
    <%= f.text_field :name %>
  </p>
  <p>
    <%= f.label :Responsable %><br />
    <%= f.text_field :responsible %>
  </p>
  <p>
    <%= f.label :Ubicacion %><br />
    <%= f.text_field :location %>
  </p>
  <p>
    <%= f.label :Telefono %><br />
    <%= f.text_field :phone %>
  </p>
  <p>
    <%= f.label :Celular %><br />
    <%= f.text_field :movil %>
  </p>
  <p>
    <%= f.label :Email %><br />
    <%= f.text_field :email %>
  </p>
  <p>
    <%= f.label :Transporte %><br />
    <%= f.text_field :transport %>
  </p>
  <p>
    <%= f.label :Descripcion %><br />
    <%= f.text_area :description %>
  </p>
  <p>
    <%= f.submit 'Create' %>
  </p>
<% end %>

<%= link_to 'Back', social_works_path %>