<%if current_user%>
<p>
  <b>Nombre de Usuario</b>
  <%=h @user.username %>
</p>

<p>
  <b>E-mail</b>
  <%=h @user.email %>
</p>

<p>
  <b>Rol usuario</b>
  <%=h User::USERS_TYPES[@user.role] %>
</p>


<%= link_to 'Editar', edit_user_path(@user) %> |
<%= link_to 'Atrás', users_path %>

<%else%>
<h2>No puede realizar esta acción, por favor contáctese con el administrador del sistema</h2>
<%end%>