    <div id="content-wrap">
      <%= render :partial => "lateralMenu" %>
       <% if ((@socialWorks.count>0)||(@campaings.count>0))%>
      <script language="JavaScript" align="Center">
        window.onload = function () {
        $(".slidetabs").data("slideshow").play();
      }
      </script>
      <div id="content" >
                      <script>
                        $(function() {	
                            $("#jqt3").addClass("active");
                            $("<div/>", { id: 'footercrumb' }).appendTo($("#content")).html($("#breadcrumb").html());
                        });
                      </script>
              <style>	
              ul.css-tabs li {
                  list-style-image:none !important;
                  list-style-type:none !important;
                  margin:0 !important;
              }

              div.css-panes h2 {
                  margin-top:0px;		
              }

              h1 {
                  background-position:0 0px;		
              } 
              </style>
          <%= stylesheet_link_tag "tabs-slideshow"  %>
      <a id="backward">prev</a>
      <div class="images" >
            <%
          @i=0
          @c=0
          @s=0
          @link = ""
          while (@i < 7) &&((@campaings.count>@c)||(@s<@socialWorks.count)) do %>
              <div style="display: none;" >
            <%  if((@s<@socialWorks.count)&&(@c<@campaings.count))
                  if(@socialWorks[@s].created_at>@campaings[@c].date_ini)
                    @s1=@socialWorks[@s] %>
                   <%= link_to @s1.social_work,@s1 %>
                   <% @s+=1
                  else
                    @c1 = @campaings[@c] %>
                   <%= link_to @c1.name,@c1 %>
                   <% @c+=1
                  end
                else
                  if(@s<@socialWorks.count)
                      @s1=@socialWorks[@s] %>
                     <%= link_to @s1.social_work,@s1 %>
                     <% @s+=1
                  end
                  if(@c<@campaings.count)
                      @c1 = @campaings[@c] %>
                     <%= link_to @c1.name,@c1 %>
                     <% @c+=1
                  end
              end
              @link = "#{@link} <a class='' href='#' >*</a>"
          @i=@i+1
           %>
                <img style="float:left;margin:0 30px 20px 0" src="/notice/notice_photo/0000/0001/captura.JPG" alt="Flying screens" height="200" width="400"/>
       </div>
       <% end %>
         </div>
<a id="forward">next</a>
<div class="slidetabs">
         <%= @link%>
</div>

<div align="center" style="clear: both; margin: 0px 0pt; text-align: center; padding-right: 0px;">
<button onclick='$(".slidetabs").data("slideshow").play();'>Play</button>
<button onclick='$(".slidetabs").data("slideshow").stop();'>Stop</button>
</div>
<script language="JavaScript">
$(function() {
$(".slidetabs").tabs(".images > div", {
	effect: 'fade',
	fadeOutSpeed: "slow",
	rotate: true
}).slideshow();
});
</script>
</div>
      <% end %>
        


    </div>