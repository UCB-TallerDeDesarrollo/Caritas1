<h1>Listing parishes</h1>

<table>
  <tr>
    <th>Parish name</th>
    <th>Ubication</th>
    <th>Transport</th>
    <th>Telephone</th>
    <th>Pastor name</th>
  </tr>

<% @parishes.each do |parish| %>
  <tr>
    <td><%=h parish.parish_name %></td>
    <td><%=h parish.ubication %></td>
    <td><%=h parish.transport %></td>
    <td><%=h parish.telephone %></td>
    <td><%=h parish.pastor_name %></td>
    <td><%= link_to 'Show', parish %></td>
    <td><%= link_to 'Edit', edit_parish_path(parish) %></td>
    <td><%= link_to 'Destroy', parish, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New parish', new_parish_path %>